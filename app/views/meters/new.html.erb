<h1 class="h3">New Meter</h1>
<div class="row justify-content-center">
  <%= form_with(model: @meter, scope: :equipment, url: meters_path, class: "col-3") do |f| %>
    <%= f.label :serial, class: "form-label" %>
    <%= f.text_field :serial, class: "mb-4 form-control" %>
    <%= f.label :techid, class: "form-label" %>
    <%= f.text_field :techid, class: "mb-4 form-control" %>
    <%= f.submit %>
  <% end %>
  <!--- <div class="col-3"></div> -->
</div>